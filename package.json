  "version": "0.0.1",
  "private": true,
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --configuration=development --localize && sh scripts/post-build.sh",
    "build:prod": "ng build --localize && sh scripts/post-build.sh",
    "build:stats": "ng build --stats-json",
