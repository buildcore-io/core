{
  "name": "soonaverse-functions",
  "scripts": {
    "lint": "eslint --ext .js,.ts .",
    "build": "tsc",
    "serve": "tsc --watch & npm run emulate",
    "emulate": "firebase -c ../firebase.json emulators:start --only database,functions,firestore,ui",
    "shell": "npm run build && firebase functions:shell",
    "start": "npm run shell",
    "test": "jest test/",
    "test:watch": "jest test/ --watch-all --detect-open-handles",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log"
  },
  "engines": {
    "node": "14"
  },
  "main": "lib/src/index.js",
  "dependencies": {
    "firebase-admin": "^9.12.0",
    "firebase-functions": "^3.15.7",
    "lodash": "^4.17.21",
    "web3": "^1.6.0",
    "web3-token": "^0.1.32"
  },
  "devDependencies": {
    "@types/chai": "^4.2.22",
    "@types/lodash": "^4.14.176",
    "@typescript-eslint/eslint-plugin": "^3.9.1",
    "@typescript-eslint/parser": "^3.10.1",
    "babel-jest": "^24.9.0",
    "eslint": "^7.6.0",
    "eslint-config-google": "^0.14.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.2",
    "firebase-functions-test": "^0.2.3",
    "firebase-tools": "^9.21.0",
    "jest": "^24.9.0",
    "ts-jest": "^24.1.0",
    "ts-node": "^8.4.1",
    "ts-sinon": "^2.0.2",
    "typescript": "^3.8.0"
  },
  "private": true
}
