{
  "name": "soonaverse-functions",
  "scripts": {
    "lint": "eslint --ext .js,.ts .",
    "build": "tsc",
    "serve": "tsc --watch & firebase -c ../firebase.json emulators:start --only database,functions,firestore,ui",
    "shell": "npm run build && firebase functions:shell",
    "start": "npm run shell",
    "test": "jest test/",
    "test:watch": "jest test/ --watch-all --detect-open-handles",
    "deploy": "firebase deploy --only functions -f",
    "logs": "firebase functions:log"
  },
  "engines": {
    "node": "14"
  },
  "main": "lib/src/index.js",
  "dependencies": {
    "firebase-admin": "10.0.0",
    "firebase-functions": "3.15.7",
    "lodash": "4.17.21",
    "uuid": "^8.3.2",
    "web3": "1.6.0",
    "web3-token": "0.1.32"
  },
  "devDependencies": {
    "@types/chai": "4.2.22",
    "@types/lodash": "4.14.176",
    "@types/uuid": "^8.3.1",
    "@typescript-eslint/eslint-plugin": "5.3.0",
    "@typescript-eslint/parser": "5.3.0",
    "babel-jest": "27.3.1",
    "eslint": "8.1.0",
    "eslint-config-google": "0.14.0",
    "eslint-import-resolver-typescript": "2.5.0",
    "eslint-plugin-import": "2.25.2",
    "firebase-functions-test": "0.3.3",
    "firebase-tools": "9.21.0",
    "jest": "27.3.1",
    "ts-jest": "27.0.7",
    "ts-node": "10.4.0",
    "ts-sinon": "2.0.2",
    "typescript": "4.4.4"
  },
  "private": true
}
