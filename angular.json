{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "cli": {
    "analytics": false
  },
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "soonaverse": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:application": {
          "strict": true
        },
        "@schematics/angular:component": {
          "style": "less",
          "changeDetection": "OnPush"
        },
        "@ngneat/spectator:spectator-component": {
          "jest": true
        }
      },
      "root": "",
      "sourceRoot": "src",
      "prefix": "wen",
      "i18n": {
        "sourceLocale": "en",
        "locales": {
          "cs": {
            "translation": "src/locale/messages.cs-CZ.xlf"
          },
          "de": {
            "translation": "src/locale/messages.de-DE.xlf"
          },
          "es": {
            "translation": "src/locale/messages.es-ES.xlf"
          },
          "fr": {
            "translation": "src/locale/messages.fr-FR.xlf"
          },
          "it": {
            "translation": "src/locale/messages.it-IT.xlf"
          },
          "ja": {
            "translation": "src/locale/messages.ja-JP.xlf"
          },
          "ko": {
            "translation": "src/locale/messages.ko-KR.xlf"
          },
          "nl": {
            "translation": "src/locale/messages.nl-NL.xlf"
          },
          "pl": {
            "translation": "src/locale/messages.pl-PL.xlf"
          },
          "pt-BR": {
            "translation": "src/locale/messages.pt-BR.xlf"
          },
          "pt-PT": {
            "translation": "src/locale/messages.pt-PT.xlf"
          },
          "ru": {
            "translation": "src/locale/messages.ru-RU.xlf"
          },
          "tr": {
            "translation": "src/locale/messages.tr-TR.xlf"
          },
          "uk": {
            "translation": "src/locale/messages.uk-UA.xlf"
          },
          "zh-CN": {
            "translation": "src/locale/messages.zh-CN.xlf"
          },
          "zh-TW": {
            "translation": "src/locale/messages.zh-TW.xlf"
          }
        }
      },
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/soonaverse",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.app.json",
            "inlineStyleLanguage": "less",
            "assets": [
              "src/favicon.ico",
              "src/maintenance.html",
              "src/assets",
              {
                "glob":"**/*",
                "input":"./node_modules/@ant-design/icons-angular/src/inline-svg/",
                "output":"/assets/"
              }
            ],
            "styles": ["src/theme/styles.less"],
            "scripts": [
              "node_modules/apexcharts/dist/apexcharts.min.js"
            ]
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "10mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            },
            "cs": {
              "localize": ["cs"],
              "i18nMissingTranslation": "warning"
            },
            "de": {
              "localize": ["de"],
              "i18nMissingTranslation": "warning"
            },
            "es": {
              "localize": ["es"],
              "i18nMissingTranslation": "warning"
            },
            "fr": {
              "localize": ["fr"],
              "i18nMissingTranslation": "warning"
            },
            "it": {
              "localize": ["it"],
              "i18nMissingTranslation": "warning"
            },
            "ja": {
              "localize": ["ja"],
              "i18nMissingTranslation": "warning"
            },
            "ko": {
              "localize": ["ko"],
              "i18nMissingTranslation": "warning"
            },

            "nl": {
              "localize": ["nl"],
              "i18nMissingTranslation": "warning"
            },
            "pl": {
              "localize": ["pl"],
              "i18nMissingTranslation": "warning"
            },
            "pt-BR": {
              "localize": ["pt-BR"],
              "i18nMissingTranslation": "warning"
            },
            "pt-PT": {
              "localize": ["pt-PT"],
              "i18nMissingTranslation": "warning"
            },
            "tr": {
              "localize": ["tr"],
              "i18nMissingTranslation": "warning"
            },
            "uk": {
              "localize": ["uk"],
              "i18nMissingTranslation": "warning"
            },
            "zh-CN": {
              "localize": ["zh-CN"],
              "i18nMissingTranslation": "warning"
            },
            "zh-TW": {
              "localize": ["zh-TW"],
              "i18nMissingTranslation": "warning"
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "test:build:production"
            },
            "development": {
              "browserTarget": "soonaverse:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "soonaverse:build"
          }
        },
        "test": {
          "builder": "@angular-builders/jest:run",
          "options": {
            "polyfills": ["src/polyfills.ts"],
            "tsConfig": "tsconfig.spec.json",
            "assets": ["src/favicon.ico", "src/assets"],
            "styles": [],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": ["src/**/*.ts", "src/**/*.html"]
          }
        }
      }
    }
  },
  "defaultProject": "soonaverse"
}
