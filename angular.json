{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "cli": {
    "analytics": false
  },
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "soonaverse": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:application": {
          "strict": true
        },
        "@schematics/angular:component": {
          "style": "less",
          "changeDetection": "OnPush"
        },
        "@ngneat/spectator:spectator-component": {
          "jest": true
        }
      },
      "root": "",
      "sourceRoot": "src",
      "prefix": "wen",
      "i18n": {
        "sourceLocale": "en",
        "locales": {
          "cs-CZ": {
            "translation": "src/locale/messages.cs-CZ.xlf"
          },
          "de-DE": {
            "translation": "src/locale/messages.de-DE.xlf"
          },
          "es-ES": {
            "translation": "src/locale/messages.es-ES.xlf"
          },
          "fr-FR": {
            "translation": "src/locale/messages.fr-FR.xlf"
          },
          "it-IT": {
            "translation": "src/locale/messages.it-IT.xlf"
          },
          "ja-JP": {
            "translation": "src/locale/messages.ja-JP.xlf"
          },
          "pl-PL": {
            "translation": "src/locale/messages.pl-PL.xlf"
          },
          "pt-BR": {
            "translation": "src/locale/messages.pt-BR.xlf"
          },
          "ru-RU": {
            "translation": "src/locale/messages.ru-RU.xlf"
          },
          "tr-TR": {
            "translation": "src/locale/messages.tr-TR.xlf"
          },
          "uk-UA": {
            "translation": "src/locale/messages.uk-UA.xlf"
          },
          "zh-CN": {
            "translation": "src/locale/messages.zh-CN.xlf"
          }
        }
      },
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/soonaverse",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.app.json",
            "inlineStyleLanguage": "less",
            "assets": [
              "src/favicon.ico",
              "src/maintenance.html",
              "src/assets",
              {
                "glob":"**/*",
                "input":"./node_modules/@ant-design/icons-angular/src/inline-svg/",
                "output":"/assets/"
              }
            ],
            "styles": ["src/theme/styles.less"],
            "scripts": [
              "node_modules/apexcharts/dist/apexcharts.min.js"
            ]
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "10mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            },
            "cs": {
              "localize": ["cs-CZ"],
              "outputPath": "dist/cs/",
              "i18nMissingTranslation": "error"
            },
            "de": {
              "localize": ["de-DE"],
              "outputPath": "dist/de/",
              "i18nMissingTranslation": "error"
            },
            "es": {
              "localize": ["es-ES"],
              "outputPath": "dist/es/",
              "i18nMissingTranslation": "error"
            },
            "fr": {
              "localize": ["fr-FR"],
              "outputPath": "dist/fr/",
              "i18nMissingTranslation": "error"
            },
            "it": {
              "localize": ["it-IT"],
              "outputPath": "dist/it/",
              "i18nMissingTranslation": "error"
            },
            "ja": {
              "localize": ["ja-JP"],
              "outputPath": "dist/ja/",
              "i18nMissingTranslation": "error"
            },
            "pl": {
              "localize": ["pl-PL"],
              "outputPath": "dist/pl/",
              "i18nMissingTranslation": "error"
            },
            "pt-BR": {
              "localize": ["pt-BR"],
              "outputPath": "dist/pt-BR/",
              "i18nMissingTranslation": "error"
            },
            "pt-PT": {
              "localize": ["pt-PT"],
              "outputPath": "dist/pt-PT/",
              "i18nMissingTranslation": "error"
            },
            "tr": {
              "localize": ["tr-TR"],
              "outputPath": "dist/tr/",
              "i18nMissingTranslation": "error"
            },
            "uk": {
              "localize": ["uk-UA"],
              "outputPath": "dist/uk/",
              "i18nMissingTranslation": "error"
            },
            "zh": {
              "localize": ["zh-CN"],
              "outputPath": "dist/zh/",
              "i18nMissingTranslation": "error"
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "test:build:production"
            },
            "development": {
              "browserTarget": "soonaverse:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "soonaverse:build"
          }
        },
        "test": {
          "builder": "@angular-builders/jest:run",
          "options": {
            "polyfills": ["src/polyfills.ts"],
            "tsConfig": "tsconfig.spec.json",
            "assets": ["src/favicon.ico", "src/assets"],
            "styles": [],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": ["src/**/*.ts", "src/**/*.html"]
          }
        }
      }
    }
  },
  "defaultProject": "soonaverse"
}
